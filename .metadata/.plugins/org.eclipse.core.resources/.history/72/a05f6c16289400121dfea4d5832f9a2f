package com.gottagged380;


public class JoinGame {

		private boolean join;
		private String email;
		
		public String getEmail(){
			 return email;
		 }
		
		public void setEmail(String email){
			this.email = email;
		}
		
		public void setJoin(boolean join){
			this.join = join;
		}
		
		public boolean getJoin(){
			return join;
		}
}
